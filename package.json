{
  "name": "cypress-tool",
  "version": "0.0.0",
  "description": "Tool to help update/fix Cypress installs.",
  "main": "lib/index.js",
  "bin": "bin.js",
  "scripts": {
    "build": "babel src -d lib",
    "start": "node bin.js",
    "clean": "rimraf lib",
    "format": "prettier --write \"**/*.{js,json}\"",
    "compile": "pkg . --targets node10-macos-x64,node10-win-x64"
  },
  "keywords": [],
  "author": "Alex Lee <www.alexlee.dev>",
  "pkg": {
    "scripts": [
      "lib/**/*.js"
    ],
    "assets": [
      "node_modules/figlet/fonts/slant.flf"
    ]
  },
  "dependencies": {
    "@sentry/node": "^5.13.0",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "core-js": "^3.6.4",
    "figlet": "^1.3.0",
    "https-proxy-agent": "^5.0.0",
    "inquirer": "^7.0.5",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.3",
    "progress": "^2.0.3",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.2",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "pkg": "^4.4.4",
    "prettier": "^1.19.1"
  }
}
